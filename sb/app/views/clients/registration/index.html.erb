<% provide :title, "クライアント登録" %>
<% provide :breadcrumb_items, "Home クライアント" %>

<div class="mx-auto mt-5" style="width: 500px;">
    <div class="mb-3">
        クライアント登録Excelを選択してください
    </div>
    <%= form_with url: clients_registration_upload_path, method: "post", local: true, multipart: true do %>
        <div class="form-group">
            <%= render partial: "shared/file_upload", locals: { name: "input_file" } %>
        </div>
        <div class="form-group text-right">
            <%= submit_tag "送信", class: "btn btn-primary" %>
        </div>
    <% end %>
</div>
